{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".", // Ensure baseUrl is set
    "paths": {
      "@/*": ["./*"],
      "@screens/*": ["app/screens/*"],
      "@navigation/*": ["./app/navigation/*"] // Add this line
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
, "firebase/firebase.js"  ]
}